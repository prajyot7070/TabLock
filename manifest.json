{
    "manifest_version": 3,
    "name": "TabLock",
    "version": "1.0",
    "description": "Lock private or confidential tabs with a password",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
    "permissions": ["tabs", "storage", "activeTab","scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
    
  }